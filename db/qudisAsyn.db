record(ai, "$(P)$(R)absPos1_RBV")
{
	field(DTYP, "asynFloat64")
    	field(INP,  "@asyn($(PORT),$(ADDR))ABS_POS1_VALUE")
    	field(LINR, "NO CONVERSION")
	field(PREC, "2")
	# field(EGU,  "V")
    	field(SCAN, "I/O Intr")
    	#field(SCAN, "Passive")
}
